class Animal:
    def hacerSonido(self):
        pass 
class Perro(Animal):
    def hacerSonido(self):
        return "Guau"
class Gato(Animal):
    def hacerSonido(self):
        return "Miau"
if __name__ == "__main__":
    lista_animales = [Perro(), Gato(), Perro(), Gato()]
    for animal in lista_animales:
        print(animal.hacerSonido())
